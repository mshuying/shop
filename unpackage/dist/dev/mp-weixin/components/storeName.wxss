.store {
  width: 100%;
  height: 64rpx;
  padding: 15rpx 20rpx;
  background-color: #FFFFFF;
  margin-bottom: 2rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.store .store-icon {
  font-size: 30rpx;
  color: #4ca0ff;
  margin-right: 10rpx;
}
.store .store-name {
  font-size: 22rpx;
  color: #333;
}

